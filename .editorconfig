# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

[*.rs]
indent_style = space
indent_size = 4

[args.rs]
indent_size = unset

[*.yaml]
indent_style = space
indent_size = 2

[Makefile]
indent_style = tab